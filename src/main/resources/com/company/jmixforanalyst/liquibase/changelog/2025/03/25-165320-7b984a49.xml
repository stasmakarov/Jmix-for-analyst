<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1" author="jal">
        <addColumn tableName="JAL_LEAD">
            <column name="EMAIL" type="VARCHAR(255)"/>
            <column name="PERFORMER" type="VARCHAR(255)"/>
        </addColumn>
    </changeSet>
    <changeSet id="2" author="jal">
        <dropColumn columnName="DATE_" tableName="JAL_LEAD"/>

        <dropColumn columnName="PRODUCT_ID" tableName="JAL_LEAD"/>

        <dropColumn columnName="STATUS" tableName="JAL_LEAD"/>
    </changeSet>
</databaseChangeLog>